{
    "version": 2,
    "builds": [
      {
        "src": "front/vite-project/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "back/src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/front/vite-project/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/back/src/$1"
      }
    ]
  }
  