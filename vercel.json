{
    "builds": [
      {
        "src": "front/vite-project/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "back/api/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/back/api/$1"
      }
    ]
  }
  
  