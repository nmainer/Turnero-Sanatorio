{
  "version": 2,
  "builds": [
    {
      "src": "front/dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "back/dist/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/front/dist/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/back/dist/src/index.js"
    }
  ]
}


