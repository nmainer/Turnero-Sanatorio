{
  "version": 2,
  "builds": [
    {
      "src": "vite-project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "vite-project/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vite-project/dist/$1"
    }
  ],
  "buildCommand": "npm run build"
}

  