{
    "version": 2,
    "builds": [
      {
        "src": "front/vite-project/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "front/vite-project/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "vite-project/dist/$1"
      }
    ]
  }
  